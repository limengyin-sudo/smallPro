.item{
    width: 100%;
    height: 170rpx;
    border: 1px solid #e0e0e0;
    display: flex;
    align-items: center;
}
.pic{
    width: 120rpx;
    height: 120rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
}
.detail{
    width: 300rpx;
    margin-right: 30rpx;
}
.name,
.radioName,
.listenerCount{
    display: block;
    width: 300rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.name{
    color: #333;
    font-size: 32rpx;
}
.radioName{
    font-size: 30rpx;
    color: #666;
}
.listenerCount{
    font-size: 28rpx;
    color: #999;
}
.likedCount{
    font-size: 30rpx;
    color: #666;
    margin-right: 20rpx;
}
.category{
    font-size: 32rpx;
    color: #333;
    border: 1px solid #666;
}