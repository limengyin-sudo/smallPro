.item{
    width: 100%;
    height: 160rpx;
    position: relative;
    border-bottom: 1px solid #e0e0e0;
    display: flex;
    align-items: center;
}
.pic{
    width: 100rpx;
    height: 100rpx;
    margin-left: 20rpx;
    margin-right: 20rpx;
}
.songName{
    margin-right: 50rpx;
}
.dt{
    font-size: 30rpx;
    color: #666;
    margin-right: 60rpx;
}
.songName,
.alName{
    display: inline-block;
    width: 200rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 32rpx;
    color: #333;
}